{
    "log-driver": "local",
    "log-opts": {
      "max-size": "10m"
    },
    "runtimes": {
        "nvidia": {
            "args": [],
            "path": "nvidia-container-runtime"
        }
    },
    "default-runtime": "io.containerd.runc.v2",
    "exec-opts": ["native.cgroupdriver=cgroupfs"],
    "features": {
        "containerd-snapshotter": true
    }

}
